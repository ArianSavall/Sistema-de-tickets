<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="datos.Especialidad" table="especialidad">
		<id column="id_especialidad" name="id">
			<generator class="identity" />
		</id>
		<property column="nombre" name="nombre" type="string" />
		<set table="especialidad-soporte" name="id_especialidad" outer-join="true">
		<key column="id"/>
		<many-to-many column="id_especialidad" class="datos.Soporte"/>
		</set>
	</class>
</hibernate-mapping>